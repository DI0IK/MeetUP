{
  "$schema": "https://json.schemastore.org/renovate",
  "extends": ["config:best-practices", ":semanticCommits"],
  "automergeType": "branch",
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "digest"],
      "matchCurrentVersion": "!/^0/",
      "automerge": true
    }
  ]
}
